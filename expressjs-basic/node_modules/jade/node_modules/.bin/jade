#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Admin/Desktop/NodeJS/expressjs-basic/node_modules/.pnpm/jade@1.11.0/node_modules/jade/bin/node_modules:/mnt/c/Users/Admin/Desktop/NodeJS/expressjs-basic/node_modules/.pnpm/jade@1.11.0/node_modules/jade/node_modules:/mnt/c/Users/Admin/Desktop/NodeJS/expressjs-basic/node_modules/.pnpm/jade@1.11.0/node_modules:/mnt/c/Users/Admin/Desktop/NodeJS/expressjs-basic/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Admin/Desktop/NodeJS/expressjs-basic/node_modules/.pnpm/jade@1.11.0/node_modules/jade/bin/node_modules:/mnt/c/Users/Admin/Desktop/NodeJS/expressjs-basic/node_modules/.pnpm/jade@1.11.0/node_modules/jade/node_modules:/mnt/c/Users/Admin/Desktop/NodeJS/expressjs-basic/node_modules/.pnpm/jade@1.11.0/node_modules:/mnt/c/Users/Admin/Desktop/NodeJS/expressjs-basic/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jade.js" "$@"
else
  exec node  "$basedir/../../bin/jade.js" "$@"
fi
